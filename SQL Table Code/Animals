CREATE TABLE animal (
  animalID INT PRIMARY KEY,
  date VARCHAR(50) NOT NULL,
  type VARCHAR(50) NOT NULL,
  name VARCHAR(50) NOT NULL,    dateOfBirth VARCHAR(50) NOT NULL,
  sex CHAR(1) NOT NULL,   breed VARCHAR(50) NOT NULL,
  color VARCHAR(20) NOT NULL,
  weight FLOAT NOT NULL,
  altered CHAR(1) NOT NULL,
  microchip CHAR(1) NOT NULL,
  broughtIn VARCHAR(50) NOT NULL,
  location VARCHAR(50) NOT NULL,
  owner VARCHAR(50) NOT NULL,
  phone VARCHAR(15) NOT NULL,
  address VARCHAR(100) NOT NULL,
  city VARCHAR(50) NOT NULL,
  state CHAR(2) NOT NULL,
  zipcode INT NOT NULL,
  rabiesVacc CHAR(1) NOT NULL,    rabiesYear INT NOT NULL,
  distemperVacc CHAR(1) NOT NULL,     distemperYear INT NOT NULL,
  spayedNeutered CHAR(1) NOT NULL,
  tagNumber INT,
  clinic VARCHAR(50),
  file VARCHAR(100) NOT NULL
); 
