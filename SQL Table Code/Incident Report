CREATE TABLE incidentreport (
badgeNumber INT PRIMARY KEY,
intakeNumber VARCHAR(50),
date VARCHAR(50),
time VARCHAR(50),
weather VARCHAR(50),
type VARCHAR(50),
sex VARCHAR(1),
color VARCHAR(50),
owner VARCHAR(50),
phone INT,
address VARCHAR(50),
city VARCHAR(50),
state VARCHAR(50),
zipcode INT,
description TEXT(100)
);
